        <div class="section primary-section " id="apply">
                <div class="triangle"></div>
            <div class="container">
	                <h3>Key Benefits</h3>
	                <div class="row-fluid">
	                    <div class="span6">
	                        <ul class="skills">
	                            <li>
	                                <span class="bar" data-width="97%"></span>
